@{
    ViewData["Title"] = "Setting";
}

<form>
    <div class="form-group">
        <button type="submit" name="button" value="success" class="btn btn-primary mb-2" asp-action="Setting">重新設定</button>
        @TempData["Setting"]
    </div>
    <div class="form-group">
        <label for="exampleFormControlTextarea">內容</label>
        <textarea class="form-control" id="exampleFormControlTextarea" rows="10">
            @TempData["Customers"]
        </textarea>
    </div>
</form>